{ 
  "version": 2, 
  "rewrites": [{ 
    "source": "/(.*)", 
    "destination": "/api" 
  }],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/anime-abyss-backend.vercel.app/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }  
]
}
